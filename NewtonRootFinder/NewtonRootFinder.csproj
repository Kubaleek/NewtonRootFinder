<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<OutputType>Library</OutputType>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NoWarn>1591</NoWarn>

		<PackageId>NewtonRootFinder</PackageId>
		<Version>1.0.0</Version>
		<Authors>Kuba 'Kubaleek' Król</Authors>
		<Description>Biblioteka do znajdowania pierwiastków rzeczywistych wielomianów metodą Newtona z deflacją.</Description>
		<PackageTags>math;polynomial;newton;roots;numerics</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryUrl>https://github.com/Kubaleek/NewtonRootFinder</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
	</PropertyGroup>
</Project>
